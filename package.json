{
  "name": "accessible-typeahead",
  "version": "0.3.1",
  "description": "A typeahead component, built to be accessible.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "build": "cross-env NODE_ENV=production webpack -p --progress --display-modules",
    "test": "npm run -s lint && npm run -s test:karma",
    "test:dev": "karma start test/karma.conf.js",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "lint": "standard",
    "preversion": "npm test",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "a11y",
    "accessibility",
    "autocomplete",
    "component",
    "plugin",
    "typeahead",
    "widget"
  ],
  "license": "MIT",
  "author": "Theodor Vararu <theodor.vararu@digital.cabinet-office.gov.uk>",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "core-js": "^2.4.1",
    "cross-env": "^3.1.3",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.0.0",
    "phantomjs-prebuilt": "^2.1.12",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.5",
    "standard": "^8.6.0",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "webpack-sources": "^0.1.4"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "preact": "^6.0.0"
  },
  "main": "dist/accessible-typeahead.min.js",
  "standard": {
    "parser": "babel-eslint"
  }
}
