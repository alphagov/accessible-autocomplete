{
  "name": "ng-accessible-autocomplete",
  "version": "3.0.0",
  "main": "dist/accessible-autocomplete.min.js",
  "style": "dist/accessible-autocomplete.min.css",
  "description": "An autocomplete component, built to be accessible.",
  "repository": "alphagov/accessible-autocomplete",
  "author": "Government Digital Service (https://www.gov.uk/government/organisations/government-digital-service)",
  "license": "MIT",
  "keywords": [
    "a11y",
    "accessibility",
    "autocomplete",
    "component",
    "plugin",
    "typeahead",
    "widget"
  ],
  "scripts": {
    "build:css": "csso src/autocomplete.css -o dist/accessible-autocomplete.min.css",
    "build:js": "cross-env NODE_ENV=production webpack --progress",
    "build": "run-s 'build:js' 'build:css'",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --progress",
    "karma:dev": "cross-env NODE_ENV=test karma start test/karma.config.js",
    "karma": "npm run karma:dev -- --single-run",
    "preversion": "npm test",
    "standard": "standard",
    "standard:fix": "standard --fix",
    "test": "run-p standard karma wdio",
    "version": "npm run build && git add -A dist",
    "wdio:test": "cross-env NODE_ENV=test wdio test/wdio.config.js",
    "wdio": "npm run build wdio:test && git checkout dist/"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run build && node scripts/check-staged.js"
    }
  },
  "dependencies": {
    "core-js": "^3.26.1",
    "preact": "^10.11.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.2",
    "@babel/plugin-transform-member-expression-literals": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/plugin-transform-property-literals": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@wdio/cli": "^7.26.0",
    "@wdio/local-runner": "^7.26.0",
    "@wdio/mocha-framework": "^7.26.0",
    "@wdio/sauce-service": "^7.26.0",
    "@wdio/spec-reporter": "^7.26.0",
    "@wdio/static-server-service": "^7.26.0",
    "@wdio/sync": "^7.26.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.7",
    "chalk": "^5.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "csso-cli": "^4.0.1",
    "dotenv": "^16.0.3",
    "husky": "^8.0.2",
    "karma": "^6.4.1",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^19.2.2",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "sinon": "^14.0.2",
    "sinon-chai": "^3.7.0",
    "source-map-loader": "^4.0.1",
    "standard": "^17.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "webdriverio": "^7.26.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "browserslist": [
    ">0.1%",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Samsung versions",
    "Safari >= 9",
    "ie 8-11",
    "iOS >= 9"
  ],
  "standard": {}
}
