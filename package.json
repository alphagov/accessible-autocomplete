{
  "name": "accessible-autocomplete",
  "version": "1.6.1",
  "main": "dist/accessible-autocomplete.min.js",
  "style": "dist/accessible-autocomplete.min.css",
  "description": "An autocomplete component, built to be accessible.",
  "repository": "alphagov/accessible-autocomplete",
  "author": "Government Digital Service (https://www.gov.uk/government/organisations/government-digital-service)",
  "license": "MIT",
  "keywords": [
    "a11y",
    "accessibility",
    "autocomplete",
    "component",
    "plugin",
    "typeahead",
    "widget"
  ],
  "scripts": {
    "build:css": "csso src/autocomplete.css -o dist/accessible-autocomplete.min.css",
    "build:js": "cross-env NODE_ENV=production webpack -p --progress --display-modules",
    "build": "run-s 'build:js' 'build:css'",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "karma:dev": "karma start test/karma.config.js",
    "karma": "npm run karma:dev -- --single-run",
    "prepush": "npm run build && node scripts/check-staged.js",
    "preversion": "npm test",
    "standard": "standard",
    "test": "run-p standard karma wdio",
    "version": "npm run build && git add -A dist",
    "wdio": "wdio test/wdio.config.js && git checkout dist/"
  },
  "dependencies": {
    "preact": "^8.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.2",
    "coveralls": "^2.13.3",
    "cross-env": "^5.2.0",
    "csso-cli": "^1.0.0",
    "dotenv": "^4.0.0",
    "husky": "^0.14.1",
    "isparta-loader": "^2.0.0",
    "karma": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.0.0",
    "npm-run-all": "^4.0.2",
    "phantomjs-prebuilt": "^2.1.12",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "sinon": "^3.0.0",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.2.1",
    "standard": "^10.0.2",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-sauce-service": "^0.4.10",
    "wdio-selenium-standalone-service": "^0.0.10",
    "wdio-spec-reporter": "^0.1.5",
    "wdio-static-server-service": "^1.0.1",
    "wdio-webpack-service": "^1.0.1",
    "webdriverio": "^4.13.0",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-sources": "^1.0.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
